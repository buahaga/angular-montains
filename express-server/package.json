{
  "name": "angular-montains-express-server",
  "version": "2.0.1",
  "description": "Node.js backend for Angular-Mountains",
  "main": "app.js",
  "scripts": {
    "start": "nodemon",
    "build": "webpack --config webpack.config.js",
    "serve": "node ./dist/app.js",
    "test": "jest --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "/"
    ],
    "testMatch": [
      "**/jest-tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "JWT",
    "Node.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^3.0.10"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jsonwebtoken": "^7.2.5",
    "chai": "^4.1.2",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7"
  }
}
